%-------------------------------------------------------------------------------%
\begin{frame}{Average treatment effect}

\begin{wideitemize}
\item Individual effect: $\tau_i = Y_i(1) - Y_i(0)$.\pause
\item Average treatment effect: $\tau = \E[\tau_i]$.\pause
\end{wideitemize}

\vskip 0.3cm
\begin{align*}
\E[\tau_i] & = \E\bigl[Y_i(1) - Y_i(0)\bigr] \\
& = \E\bigl[Y_i(1)\bigr] - \E\bigl[Y_i(0)\bigr].
\end{align*}
\pause

\vskip 0.3cm
\begin{wideitemize}
\item We need to make some assumptions to link observed data to $\E[Y_i(d)]$.
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item Tease the decomposition and assumptions coming next week.
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Sharp bounds for the ATE}

\begin{wideitemize}
\item Let $Y_i(0)$, $Y_i(1)$, and $D_i$ be random variables such that, for all $d \in \{0,1\}$,
$\Supp[Y_i(d)] \subseteq [a,b]$ and $\Supp[D_i]=\{0,1\}$
\item Let $Y_i = Y_i(1) D_i + Y_i(0) (1-D_i)$ and $\tau_i = Y_i(1)-Y_i(0)$.
\item 
Then:
\begin{align*}
\E[\tau_i] \in
\Bigr\{
&\E[Y_i \mid D_i=1]\Pr[D_i=1] + a\Pr[D_i=0]
-\\
& \qquad \bigl(\E[Y_i \mid D_i=0]\Pr[D_i=0] + b\Pr[D_i=1]\bigr),\\
&\E[Y_i \mid D_i=1]\Pr[D_i=1] + b\Pr[D_i=0]
-\\
&\qquad \bigl(\E[Y_i \mid D_i=0]\Pr[D_i=0] + a\Pr[D_i=1]\bigr)
\Bigr\}.
\end{align*}
\end{wideitemize}
\end{frame}

%-------------------------------------------------------------------------------%
\begin{frame}{Estimating sharp bounds for the ATE}

\begin{wideitemize}
\item Given $n$ i.i.d. observations of $(Y_i, D_i)$: 
\item The plug-in estimator for the upper bound is:
\[
\frac{1}{n}\sum_{i=1}^n \Bigl(
Y_i D_i + a(1-D_i) - Y_i(1-D_i) - bD_i
\Bigr)
\]
\item and the plug-in estimator for the lower bound is:
\[
\frac{1}{n}\sum_{i=1}^n \Bigl(
Y_i D_i + b(1-D_i) - Y_i(1-D_i) - aD_i
\Bigr).
\]
\item These estimators are unbiased and consistent for the lower and upper bounds respectively for $\E[\tau_i]$.
\end{wideitemize}
\end{frame}

