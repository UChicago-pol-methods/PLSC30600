%-------------------------------------------------------------------------------%

\begin{frame}{Missing data as the identification problem}

\begin{wideitemize}
\item We observe $(Y_i^*, R_i)$, not $Y_i$.\textsuperscript{\textdagger}\pause
\item Without assumptions on $(Y_i, R_i)$, $\E[Y_i]$ is not identified.\pause
\item A parameter $\theta(P)$ is identified if all data-generating processes consistent with the observed data imply the same value of $\theta$.
\end{wideitemize} 

\textsuperscript{\textdagger} Note target.
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item Connect to the missing-data science table.
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Bounds under minimal assumptions}

\begin{wideitemize}
\item If $Y_i \subseteq [a,b]$, we can bound $\E[Y_i]$.\pause
\item Lower bound: set missing values to $a$.\pause
\item Upper bound: set missing values to $b$.\pause
\item Tighter bounds require stronger assumptions.
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item xxx
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Sharp bounds for the expected value}

\begin{wideitemize}
\item Let $Y_i$ and $R_i$ be random variables with $\Supp[Y_i] \subseteq [a,b]$ and $\Supp[R_i]=\{0,1\}$.
\item Let $Y_i^* = Y_i R_i + (-99)(1 - R_i)$. 
\item
Then:
\begin{align*}
\E[Y_i] \in
\Bigr\{
&\E[Y_i^* \mid R_i=1]\Pr[R_i=1] + a\Pr[R_i=0],\\
& \qquad \bigl(\E[Y_i^* \mid R_i=1]\Pr[R_i=1] + b\Pr[R_i=0]\bigr)
\Bigr\}.
\end{align*}
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item xxx
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Estimating sharp bounds for the expected value}

\begin{wideitemize}
\item Given $n$ i.i.d. observations of $(Y_i^*, R_i)$: 
\item the plug-in estimator for the upper bound is:
\[
\frac{1}{n} \sum_{i=1}^n \Bigl(Y_i^* R_i + a(1-R_i)\Bigr)
\]
\item and the plug-in estimator for the lower bound is:
\[
\frac{1}{n} \sum_{i=1}^n \Bigl(Y_i^* R_i + b(1-R_i)\Bigr).
\]
\item These estimators are unbiased and consistent for their respective bounds. 
\end{wideitemize}
\end{frame}

