9 & A & 1 & \textcolor{Violator1}{1} & \textcolor{Violator1}{1} \\
10 & B & \textcolor{red!40}{?} & \textcolor{Violator1}{0} & \textcolor{red!40}{1} \\
\end{tabular}
\end{table}

\vspace{0.3cm}
\[
\widehat{\E}[Y_i \mid R_i=1, X_i=\text{A}] = 1/3, \quad
\widehat{\E}[Y_i \mid R_i=1, X_i=\text{B}] = 1
\]

\[
\widehat{\E}[Y_i(0)] =
\frac{5}{10}\widehat{\E}[Y_i \mid R_i=1, X_i=\text{A}] +
\frac{5}{10}\widehat{\E}[Y_i \mid R_i=1, X_i=\text{B}]
= 0.667
\]
\[
\text{Imputed sample mean} = \frac{0+1/3+1+1+0+1/3+1+1+1+1}{10} = 0.667
\]
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item MAR uses subgroup means to impute missing values.
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Potential outcomes are missing data}

\begin{wideitemize}
\item We observe $Y_i(1)$ only if $D_i=1$ and $Y_i(0)$ only if $D_i=0$.\pause
\item The unobserved potential outcome is missing data.\pause
\item Identification requires assumptions about assignment.
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
