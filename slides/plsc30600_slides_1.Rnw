%-------------------------------------------------------------------------------%
\begin{frame}{MAR: Missing at Random}

\begin{wideitemize}
\item Let $Y_i$ and $R_i$ be random variables with $\Supp[R_i]=\{0,1\}$.\pause
\item Let $Y_i^* = Y_i R_i + (-99)(1 - R_i)$, and let $X_i$ be a random vector.\pause
\item $Y_i$ is MAR conditional on $X_i$ if:\pause
\begin{wideitemize}
\item $Y_i \perp R_i \mid X_i$ (independence of outcome and response conditional on $X_i$).\pause
\item $\exists \varepsilon > 0$ such that $\Pr[R_i=1 \mid X_i] > \varepsilon$ (nonzero probability of response conditional on $X_i$).
\end{wideitemize}
\item This is a \textit{statistical assumption.}
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item Bridge to Week 2 and propensity score logic.
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Positivity conditions under MCAR vs.\ MAR}

\textbf{Missing Completely at Random (MCAR)}
  \[
  \Pr[R_i = 1] > 0.
  \]
\begin{itemize}
  \item Interpretation: we must observe \emph{some} outcomes.
\end{itemize}

\vspace{1em}

\textbf{Missing at Random (MAR)}

  \[
  \Pr[R_i = 1 \mid X_i = x] \ge \varepsilon > 0
  \quad \forall x \in \Supp[X].
  \]

\begin{itemize}
  \item Interpretation: we must observe outcomes for \emph{every covariate stratum}.
\end{itemize}

\vspace{0.75em}

\begin{itemize}
\item MCAR asks: ``Do we observe \textit{anyone}?''
\item MAR asks: ``Do we observe \textit{someone like everyone}?''
\end{itemize}
\end{frame}
%-------------------------------------------------------------------------------%
\begin{frame}{Expected value under MAR}

\begin{wideitemize}
\item Under MAR, the expected value is identified by conditioning on $X_i$:\pause
\end{wideitemize}

\begin{align*}
\E[Y_i] & = \sum_x \E[Y_i^* \mid R_i=1, X_i=x] \Pr[X_i=x] \\
& = \E\bigl[ \E[Y_i \mid R_i=1, X_i] \bigr]
\end{align*}
\end{frame}

%-------------------------------------------------------------------------------%
\begin{frame}{MAR plug-in estimator}

\begin{wideitemize}
\item Sample analogue (plug-in):\pause
\end{wideitemize}

\[
\widehat{\E}[Y_i] =
\sum_{x \in \Supp[X_i]}
\widehat{\E}[Y_i \mid R_i=1, X_i=x]\,
\widehat{\Pr}[X_i=x]
\]

\[
\widehat{\E}[Y_i \mid R_i=1, X_i=x]
=
\frac{\sum_{i=1}^n Y_i^* R_i \cdot \mathbbm{1}(X_i=x)}
{\sum_{i=1}^n R_i \cdot \mathbbm{1}(X_i=x)}
\]

\end{frame}

