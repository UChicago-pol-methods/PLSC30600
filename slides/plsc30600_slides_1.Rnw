%-------------------------------------------------------------------------------%
\begin{frame}{Notation: missing data}

\begin{wideitemize}
\item $\Supp[R] = \{0, 1\}$; $R_i = 1$ if $Y_i$ is observed, $0$ otherwise.
\begin{align*}
Y^*_i & = 
\begin{cases}
-99 & : R_i = 0 \\
Y_i &: R_i = 1
\end{cases}\\
\text{equivalent to the \textit{switching equation}:}\\
& = Y_i  R_i + (-99)  (1-R_i).
\end{align*}
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item xxx
\end{wideitemize}

}
%-------------------------------------------------------------------------------%
\begin{frame}{Notation: potential outcomes}

\begin{wideitemize}
\item $\Supp[D] = \{0, 1\}$; $D_i = 1$ if $Y_i$ is treated, $0$ otherwise.
\begin{align*}
Y_i & = 
\begin{cases}
Y_i(0) & : D_i = 0 \\
Y_i(1) &: D_i = 1
\end{cases}\\
\text{equivalent to the \textit{switching equation}:}\\
& = Y_i(1)  D_i + Y_i(0)  (1-D_i).
\end{align*}
\end{wideitemize}
\end{frame}

