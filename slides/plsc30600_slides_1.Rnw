%-------------------------------------------------------------------------------%
\begin{frame}{Interval identification}

\begin{wideitemize}
\item Our bounds are $[0.3, 0.8]$ for this example.\pause
\item Without further assumptions, the bounds are the best we can do.\pause
\item $\E[Y_i]$ is \textbf{partially identified} (set identified), not point identified.\pause
\item Even with full knowledge of $(Y_i^*, R_i)$, the exact value of $\E[Y_i]$ is not identified.
\end{wideitemize}
\end{frame}

%-------------------------------------------------------------------------------%
\begin{frame}{The Law of Decreasing Credibility \cite{manski_2003}}

\begin{quote}
The Law of Decreasing Credibility: The credibility of inference decreases with
the strength of the assumptions maintained. 
\end{quote}
\end{frame}

%-------------------------------------------------------------------------------%
\begin{frame}{MCAR: Missing Completely at Random}

\begin{wideitemize}
\item Let $Y_i$ and $R_i$ be random variables with $\Supp[R_i]=\{0,1\}$.\pause
\item Let $Y_i^* = Y_i R_i + (-99)(1 - R_i)$.\pause
\item $Y_i$ is MCAR if:\pause
\begin{wideitemize}
\item $Y_i \perp R_i$ (independence of outcome and response).\pause
\item $\Pr[R_i=1] > 0$ (nonzero probability of response).\pause
\end{wideitemize}
\item This is a \textit{statistical assumption.}
\end{wideitemize}
\end{frame}

%%%%%NOTE%%%%%
\note{
\scriptsize \singlespacing

\begin{wideitemize}
\item Emphasize MCAR is rare outside randomized nonresponse.
\end{wideitemize}

}

%-------------------------------------------------------------------------------%
\begin{frame}{Point identification}

\begin{wideitemize}
\item Stronger assumptions can deliver \textbf{point identification}.\pause
\item Under MCAR, $\E[Y_i]$ is point identified.\pause
\item The key tradeoff: tighter conclusions require stronger, (possibly) less credible assumptions.
\end{wideitemize}
\end{frame}

%-------------------------------------------------------------------------------%
\begin{frame}{Expected value under MCAR}

\begin{wideitemize}
\item Under MCAR, the expected value is identified from observed outcomes:\pause
\end{wideitemize}

\[
\E[Y_i] = \E[Y^*_i \mid R_i=1]
\]

\vskip 0.3cm
\begin{wideitemize}
\item Plug-in estimator:\pause
\end{wideitemize}

\[
\widehat{\E}[Y_i] =
\frac{\sum_{i=1}^n Y_i^* R_i}{\sum_{i=1}^n R_i}
\]
\end{frame}

